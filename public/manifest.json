{
  "manifest_version": 3,
  "name": "工具箱",
  "version": "1.0",
  "description": "这是一个 工具箱 扩展",
  "icons": {
    "16": "images/plugins.png",
    "32": "images/plugins.png",
    "48": "images/plugins.png",
    "128": "images/plugins.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": "images/plugins.png"
  },
  "background": {
    "service_worker": "scripts/background.js",
    "type": "module"
  },
  "permissions": [
    "alarms",
    "activeTab",
    "clipboardRead",
    "clipboardWrite",
    "contextMenus",
    "cookies",
    "downloads",
    "history",
    "identity",
    "management",
    "nativeMessaging",
    "notifications",
    "pageCapture",
    "storage",
    "tabs",
    "webNavigation"
  ],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+Q",
        "mac": "Command+Shift+Q"
      },
      "description": "打开扩展弹窗"
    }
  },
  "host_permissions": [
    "https://wbtest.tongchengwuxian.online/*",
    "http://localhost:1376/*"
  ]
}
